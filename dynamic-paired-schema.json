{
  "title": "Yogi Berra Quote Challenge",
  "type": "object",
  "required": [
    "full_quote"
  ],
  "properties": {
    "full_quote": {
      "type": "string",
      "title": "Select Yogism to complete",
      "enum": {{ quotes_enum|tojson }},
      "enumNames": {{ quotes_enum_names|tojson }}
    },
    "end_quote": {
      "type": "string",
      "title": "Enter how quote ends"
    },
    "prize": {
      "type": "string",
      "title": "Select prize if you are correct",
      "enum": {{ prizes_enum|tojson }},
      "enumNames": {{ prizes_enum_names|tojson }}
    }
  }
}